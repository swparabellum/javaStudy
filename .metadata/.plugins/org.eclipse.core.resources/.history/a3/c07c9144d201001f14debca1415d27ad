
public class ex1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int score = 88;
		char grade;
		
		switch(score/10) {
		case 10: case 9: grade = 'A';
		case 8: grade = 'B';
		case 7: grade ='C';
		case 6: grade='D';
		default: grade='F';
		}
		
		System.out.println("ÇÐÁ¡Àº "+grade);
	}

}
